      - name: Task
        request:
          method: POST
          header:
            - key: Content-Type
              name: Content-Type
              value: application/x-www-form-urlencoded
              type: text
          body:
            mode: urlencoded
            urlencoded:
              - key: TaskId
                value: String
                description: |-
                  **REQUIRED**

                  The ID of the task
                type: text
              - key: CompKey
                value: GUID
                description: |-
                  **REQUIRED if LocationId blank**

                  GUID for company
                type: text
              - key: LocationId
                value: String
                description: |-
                  **REQUIRED if CompKey blank**

                  ID of the location in the company for the task
                type: text
              - key: MonDayOfWeek
                value: Boolean
                description: |-
                  Task occurs on Monday

                  **At least one Day of Week is REQUIRED as true**
                type: text
              - key: TueDayOfWeek
                value: Boolean
                description: |-
                  Task occurs on Tuesday

                  **At least one Day of Week is REQUIRED as true**
                type: text
              - key: WedDayOfWeek
                value: Boolean
                description: |-
                  Task occurs on Wednesday

                  **At least one Day of Week is REQUIRED as true**
                type: text
              - key: ThuDayOfWeek
                value: Boolean
                description: |-
                  Task occurs on Thursday

                  **At least one Day of Week is REQUIRED as true**
                type: text
              - key: FriDayOfWeek
                value: Boolean
                description: |-
                  Task occurs on Friday

                  **At least one Day of Week is REQUIRED as true**
                type: text
              - key: SatDayOfWeek
                value: Boolean
                description: |-
                  Task occurs on Saturday

                  **At least one Day of Week is REQUIRED as true**
                type: text
              - key: SunDayOfWeek
                value: Boolean
                description: |-
                  Task occurs on Sunday

                  **At least one Day of Week is REQUIRED**
                type: text
              - key: Abbrev
                value: String
                description: Abbreviated name/label
                type: text
              - key: BgColor
                value: String
                description: |-
                  The background color used when displaying the task, as a hex color string (i.e. "0c0c0c", "ffffff", etc). This is typically used as a means of grouping similar staff members together.

                  NOTE: In some cases, the color black ("000000") may be represented as the string "0"
                type: text
              - key: BillSysId
                value: String
                description: |-
                  The ID of the task used for integration with an external billing system


                  Note: Field is only available via API - currently not configurable
                type: text
              - key: CallRole
                value: String
                description: The role associated with call assignments
                type: text
              - key: ContactInformation
                value: String
                description: Contact Information
                type: text
              - key: DepartmentId
                value: String
                description: The ID of the Department
                type: text
              - key: DisplayAsAvailableOnCalendarConnection
                value: Boolean
                description: Syncs to calendar as open availability
                type: text
              - key: EmrId
                value: String
                description: The external call system identifier
                type: text
              - key: EnableWaitlist
                value: Boolean
                description: Enables waitlisting for the task
                type: text
              - key: EndDate
                value: Date
                description: The end date of the task
                type: text
              - key: EnforceTimeCompatibility
                value: Boolean
                description: Enforces time compatibility
                type: text
              - key: ExtCallPriority
                value: String
                description: The external call priority
                type: text
              - key: ExtCallSysId
                value: String
                description: The ID of the task used for integration with an external call scheduling system
                type: text
              - key: Extension
                value: String
                description: The extension
                type: text
              - key: FullyCompatible
                value: Boolean
                description: The task is fully compatible with other tasks by default
                type: text
              - key: HideOnCalendarSync
                value: Boolean
                description: Will hide this task when syncing calendar
                type: text
              - key: Label
                value: Boolean
                description: If true, task will not show opens on New Admin Calendar views
                type: text
              - key: Manual
                value: Boolean
                description: 'Task is not assigned via automation '
                type: text
              - key: Name
                value: String
                description: The name of the task
                type: text
              - key: Notes
                value: String
                description: The notes for the task
                type: text
              - key: NotificationList
                value: String
                description: The notification list for the task
                type: text
              - key: OverrideSortOrder
                value: String
                description: The overridden sort order
                type: text
              - key: PayrollId
                value: String
                description: The ID of the task used for integration with an external payroll system
                type: text
              - key: PayrollUnits
                value: String
                description: The related payroll unit identifier
                type: text
              - key: RequireTimePunch
                value: Boolean
                description: Whether this task requires a time punch
                type: text
              - key: ShowEndTime
                value: Boolean
                description: The end time of the task will show with the name
                type: text
              - key: ShowOnAdminOnly
                value: Boolean
                description: Task will only show on Admin/New Admin
                type: text
              - key: TaskVisibility
                value: String
                description: 'Sets optional "TaskVisibility" value. The only accepted values will be: Show Everywhere, Personal, Show On Admin Only'
                type: text
              - key: ShowOpensOnCalendar
                value: Boolean
                description: Open shifts will show on New Admin Calendar views
                type: text
              - key: ShowStartTime
                value: Boolean
                description: The start time of the task will show with the name
                type: text
              - key: ShowTimesWhenEdited
                value: Boolean
                description: If true, the task will show times when edited
                type: text
              - key: StartDate
                value: Date
                description: The start date of the task
                type: text
              - key: SuppressRequestEmailsAndNotifications
                value: Boolean
                description: Suppresses request emails and notificaitons about the task
                type: text
              - key: SyncAsAllDayEvent
                value: Boolean
                description: Syncs task to calendar as an all day event
                type: text
              - key: TextColor
                value: String
                description: |-
                  The text color associated with the task, as a hex color string (i.e. "0c0c0c", "ffffff", etc).

                  NOTE: In some cases, the color black ("000000") may be represented as the string "0"
                type: text
              - key: TimeOffAfterShift
                value: Integer
                description: The number of hours off the staff should receive after working the shift
                type: text
              - key: Type
                value: String
                description: Determines whether the task is a Working or an Unavailable task
                type: text
              - key: IsRotationTask
                value: Boolean
                description: Task can be counted as a part of a rotation when true
                type: text
              - key: IsIgnoreHoursOffAfterTask
                value: Boolean
                type: text
          url:
            raw: https://api.qgenda.com/v2/task?dateFormat=String
            protocol: https
            host:
              - api
              - qgenda
              - com
            path:
              - v2
              - task
            query:
              - key: dateFormat
                value: String
                description: "The format we should use to interpret date fields in the request. EG “dd/MM/yyyy” or “MM/dd/yyyy” We also accept other formats like RFC1123 “DAY, dd MON yyyy hh:mm:ss” and ISO8601 “yyyy-MM-ddTHH:mm:ssZ” \nhttps://docs.microsoft.com/en-us/dotnet/standard/base-types/custom-date-and-time-format-strings"
          description: |-
            Creates a new Task with at least one Task Shift

            Response Object: [Task](#126ce210-0127-4171-842d-c401f84f5b6c)
        response: []
