            <pre class="click-to-expand-wrapper is-snippet-wrapper"><button class="btn btn-sm pull-right copy-request copy-request-modal" data-clipboard-target="#go_0_b88fba5d-e42f-40a4-b046-86f37bd3d7a4" data-before-copy="Copy to Clipboard" data-after-copy="Copied"></button><code class="language-javascript" id="go_0_b88fba5d-e42f-40a4-b046-86f37bd3d7a4">package main

import (
  "fmt"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/schedule/openshifts?companyKey=00000000-0000-0000-0000-000000000000&amp;startDate=1/1/2012&amp;endDate=1/31/2012&amp;includes=LocationTags"
  method := "GET"

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, nil)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
