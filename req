  payload := strings.NewReader("categoryName=Department&amp;tagName=Anesth&amp;companyKey=00000000-0000-0000-0000-000000000000&amp;effectiveDate=2019-01-10T00%3A00%3A00")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
