import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/dailycase/"
  method := "POST"

  payload := strings.NewReader("CompanyKey=ab480806-781b-4ff7-bc28-b0f1ad7a20a1&amp;TaskKey=6e2690c9-53c0-4e26-bd6f-2525ca10cfab&amp;TaskId=&amp;LocationName=ASC%20OR%201&amp;Date=2012-01-13T00%3A00%3A00&amp;StartTime=2012-01-13T09%3A00%3A00&amp;EndTime=2012-01-13T11%3A00%3A00&amp;Surgeon=Jane%20Smith&amp;Procedure=Colonoscopy&amp;SupStaffKey=&amp;SupStaffID%5B0%5D=MJohnson1&amp;DPStaffKey=Array%20%5BGUID%5D&amp;DPStaffID%5B0%5D=LLee&amp;CustomTextFields%5B0%5D%5B0%5D=Text%20Field%201&amp;CustomTextFields%5B0%5D%5B1%5D=Test&amp;IsCancelled=False&amp;Duration=120&amp;BaseUnits=0&amp;ModifierUnits=2&amp;TimeUnits=Number&amp;PatientFirstName=Bob&amp;PatientLastName=Lowe&amp;PatientGender=M&amp;PatientEmail=String&amp;PatientHomePhone=String&amp;PatientCellPhone=String&amp;PatientAlternatePhone=String&amp;PatientGuardianPhone=String&amp;PatientGuardianName=String&amp;PatientDOB=1950-01-30&amp;PatientMRN=String&amp;PatientClinicalNotes=String&amp;PatientExtraNotes=String&amp;PatientAddress1=String&amp;PatientAddress2=String&amp;PatientCity=String&amp;PatientState=String&amp;PatientPostalCode=String&amp;PatientPrimaryInsuranceID=String&amp;PatientSecondaryInsuranceID=String&amp;PatientSocialSecurityNumber=String&amp;PatientMaritalStatus=String&amp;EmrId=234234")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
