  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/dailycase/4686258"
  method := "PUT"

  payload := strings.NewReader("CompanyKey=ab480806-781b-4ff7-bc28-b0f1ad7a20a1&amp;TaskKey=6e2690c9-53c0-4e26-bd6f-2525ca10cfab&amp;TaskId=String&amp;LocationName=String&amp;Date=Date&amp;StartTime=Date/Time&amp;EndTime=Date/Time&amp;Surgeon=Susan%20Adkins&amp;Procedure=String&amp;SupStaffKey=Array%20%5BGUID%5D&amp;SupStaffID=Array%20%5BString%5D&amp;DPStaffKey=Array%20%5BGUID%5D&amp;DPStaffID=Array%20%5BString%5D&amp;CustomTextFields=Array%20%5B%5BStrings%5D%5D&amp;CustomCheckboxFields=Array%20%5B%5BStrings%5D%5D&amp;IsCancelled=Boolean&amp;Duration=Number&amp;BaseUnits=Number&amp;ModifierUnits=Number&amp;TimeUnits=Number&amp;PatientFirstName=String&amp;PatientLastName=String&amp;PatientGender=String&amp;PatientEmail=String&amp;PatientHomePhone=String&amp;PatientCellPhone=String&amp;PatientAlternatePhone=String&amp;PatientGuardianPhone=String&amp;PatientGuardianName=String&amp;PatientDOB=Date&amp;PatientMRN=String&amp;PatientClinicalNotes=String&amp;PatientExtraNotes=String&amp;PatientAddress1=String&amp;PatientAddress2=String&amp;PatientCity=String&amp;PatientState=String&amp;PatientPostalCode=String&amp;PatientPrimaryInsuranceID=String&amp;PatientSecondaryInsuranceID=String&amp;PatientSocialSecurityNumber=String&amp;PatientMaritalStatus=String")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
