            <pre class="click-to-expand-wrapper is-snippet-wrapper"><button class="btn btn-sm pull-right copy-request copy-request-modal" data-clipboard-target="#go_0_6a5f9fb3-d683-43c2-8e4b-76ebe58fc42a" data-before-copy="Copy to Clipboard" data-after-copy="Copied"></button><code class="language-javascript" id="go_0_6a5f9fb3-d683-43c2-8e4b-76ebe58fc42a">package main

import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/staffmember/123456"
  method := "DELETE"

  payload := strings.NewReader("CompanyKey=00000000-0000-0000-0000-000000000000&amp;LocationId=Location1234")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
