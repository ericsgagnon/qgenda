import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/staffmember/1242345"
  method := "PUT"

  payload := strings.NewReader("companyKey=00000000-0000-0000-0000-000000000000&amp;firstName=Jon&amp;lastName=Doe&amp;email=Jon.doe@sample.com&amp;middleInitial=String&amp;abbrev=JDoe1&amp;userProfile=String&amp;startDate=Date&amp;endDate=Date&amp;textColor=String&amp;bgColor=String&amp;extCallSysId=String&amp;homePhone=String&amp;mobilePhone=String&amp;pager=String&amp;addr1=String&amp;addr2=String&amp;city=String&amp;state=String&amp;zip=String&amp;isActive=Boolean&amp;npi=String&amp;billSysId=String&amp;emrId=String&amp;payrollId=String&amp;order=Decimal&amp;regHours=Decimal&amp;staffTypeKey=String&amp;billingTypeKey=String&amp;availStartMon=Time&amp;availEndMon=Time&amp;availStartTue=Time&amp;availEndTue=Time&amp;availStartWed=Time&amp;availEndWed=Time&amp;availStartThu=Time&amp;availEndThu=Time&amp;availStartFri=Time&amp;availEndFri=Time&amp;availStartSat=Time&amp;availEndSat=Time&amp;availStartSun=Time&amp;availEndSun=Time&amp;payrollStartDate=DateTime&amp;payrollEndDate=DateTime&amp;timeClockStart=DateTime&amp;timeClockEnd=DateTime&amp;regHours=Decimal&amp;dailyUnitAverage=Decimal&amp;timeClockKioskPIN=String&amp;deactivationDate=DateTime")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
