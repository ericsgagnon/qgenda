            <pre class="click-to-expand-wrapper is-snippet-wrapper"><button class="btn btn-sm pull-right copy-request copy-request-modal" data-clipboard-target="#go_0_5c3b5962-9ed8-4b4f-9036-c051ac0a2733" data-before-copy="Copy to Clipboard" data-after-copy="Copied"></button><code class="language-javascript" id="go_0_5c3b5962-9ed8-4b4f-9036-c051ac0a2733">package main

import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/tags"
  method := "POST"

  payload := strings.NewReader("CompanyKey=00000000-0000-0000-0000-000000000000&amp;TagCategory=Staff%20Type&amp;TagName=Physician&amp;IsAvailableForCreditAllocation=false&amp;IsAvailableForLocation=false&amp;IsAvailableForProfile=false&amp;IsAIsAvailableForSeries=false&amp;IsAvailableForStaff=true&amp;IsAvailableForStaffLocation=false&amp;IsAvailableForTask=false&amp;IsTTCMCategory=false&amp;IsSingleTaggingOnly=false&amp;IsPermissionCategory=false&amp;IsUsedForStats=true&amp;BackgroundColor=000000&amp;TextColor=ffffff&amp;CategoryBackgroundColor=000000&amp;CategoryTextColor=ffffff")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
