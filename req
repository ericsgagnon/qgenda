            <pre class="click-to-expand-wrapper is-snippet-wrapper"><button class="btn btn-sm pull-right copy-request copy-request-modal" data-clipboard-target="#go_0_6edc8f29-38cc-4f10-9461-ac794ea5c944" data-before-copy="Copy to Clipboard" data-after-copy="Copied"></button><code class="language-javascript" id="go_0_6edc8f29-38cc-4f10-9461-ac794ea5c944">package main

import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/staffmember/"
  method := "POST"

  payload := strings.NewReader("CompanyKey=2fbe9730-5a7f-4c97-8cd3-e25336c9cf9b&amp;StaffId=JSmith&amp;FirstName=Joe&amp;LastName=Smith&amp;Email=jsmith1@test.com&amp;MiddleInitial=String&amp;Abbrev=JSmith1&amp;UserProfile=String&amp;StartDate=Date&amp;EndDate=Date&amp;TextColor=String&amp;BgColor=String&amp;ExtCallSysId=String&amp;HomePhone=555-555-1234&amp;MobilePhone=String&amp;Pager=String&amp;Addr1=String&amp;Addr2=String&amp;City=String&amp;State=String&amp;Zip=String&amp;IsActive=Boolean&amp;Npi=234235325&amp;BillSysId=String&amp;EmrId=String&amp;PayrollId=String&amp;Order=Decimal&amp;RegHours=Decimal&amp;StaffTypeKey=String&amp;BillingTypeKey=String&amp;AvailStartMon=Time&amp;AvailEndMon=Time&amp;AvailStartTue=Time&amp;AvailEndTue=Time&amp;AvailStartWed=Time&amp;AvailEndWed=Time&amp;AvailStartThu=Time&amp;AvailEndThu=Time&amp;AvailStartFri=Time&amp;AvailEndFri=Time&amp;AvailStartSat=Time&amp;AvailEndSat=Time&amp;AvailStartSun=Time&amp;AvailEndSun=Time&amp;PayrollStartDate=DateTime&amp;PayrollEndDate=DateTime&amp;TimeClockStart=DateTime&amp;TimeClockEnd=DateTime&amp;RegHours=Decimal&amp;DailyUnitAverage=Decimal&amp;TimeClockKioskPIN=String&amp;DeactivationDate=DateTime&amp;ErpId=")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
