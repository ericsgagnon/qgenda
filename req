      <div class="heading"><span>Example Request</span></div>

          <div class="response-name"><span>Task</span></div>
      </div>
    </div>

            <pre class="click-to-expand-wrapper is-snippet-wrapper"><button class="btn btn-sm pull-right copy-request copy-request-modal" data-clipboard-target="#go_0_8cc72631-776d-4221-90ea-ebc4ec0031ec" data-before-copy="Copy to Clipboard" data-after-copy="Copied"></button><code class="language-javascript" id="go_0_8cc72631-776d-4221-90ea-ebc4ec0031ec">package main

import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/task"
  method := "POST"

  payload := strings.NewReader("TaskId=1234&amp;CompanyKey=00000000-0000-0000-0000-000000000000&amp;LocationId=5678&amp;MonDayOfWeek=true&amp;TueDayOfWeek=true&amp;WedDayOfWeek=true&amp;ThuDayOfWeek=true&amp;FriDayOfWeek=true&amp;SatDayOfWeek=true&amp;SunDayOfWeek=true&amp;Abbrev=1st%20Call&amp;BgColor=000000&amp;BillSysId=&amp;CallRole=&amp;ContactInformation=&amp;DepartmentId=&amp;DisplayAsAvailableOnCalendarConnection=false&amp;EmrId=&amp;EnableWaitlist=true&amp;EndDate=&amp;EnforceTimeCompatibility=false&amp;ExtCallPriority=&amp;ExtCallSysId=&amp;Extension=&amp;FullyCompatible=false&amp;HideOnCalendarSync=false&amp;Label=false&amp;Manual=false&amp;Name=1st%20Call&amp;Notes=&amp;NotificationList=&amp;OverrideSortOrder=&amp;PayrollId=&amp;PayrollUnits=&amp;ShowEndTime=true&amp;ShowOnAdminOnly=false&amp;ShowOpensOnCalendar=false&amp;ShowStartTime=true&amp;StartDate=&amp;SuppressRequestEmailsAndNotifications=false&amp;SyncAsAllDayEvent=false&amp;TaskId=9012&amp;RequireTimePunch=false&amp;TextColor=ffffff&amp;TimeOffAfterShift=&amp;Type=Working&amp;IsRotationTask=false")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
