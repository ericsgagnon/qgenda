            <pre class="click-to-expand-wrapper is-snippet-wrapper"><button class="btn btn-sm pull-right copy-request copy-request-modal" data-clipboard-target="#go_0_4d3dacee-d531-4eb5-ac27-2f845a106f6e" data-before-copy="Copy to Clipboard" data-after-copy="Copied"></button><code class="language-javascript" id="go_0_4d3dacee-d531-4eb5-ac27-2f845a106f6e">package main

import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/stafftarget/1010/staff"
  method := "POST"

  payload := strings.NewReader("staffId=1234&amp;targetEffectiveDate=01/01/2019&amp;targetMin=1&amp;targetIdeal=2&amp;targetMax=3&amp;targetIsActive=true")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
