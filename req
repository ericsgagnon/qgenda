import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/stafftarget"
  method := "GET"

  payload := strings.NewReader("companyKey=00000000-0000-0000-0000-000000000000&amp;name=Weekend%20Shifts&amp;isActiveMon=false&amp;isActiveTue=false&amp;isActiveWed=false&amp;isActiveThu=false&amp;isActiveFri=false&amp;isActiveSat=true&amp;isActiveSun=true&amp;occurrenceNumber%20=1&amp;occurrenceType=Months&amp;startDate=01/01/2019&amp;endDate=12/31/2019&amp;includeInAlgorithm=true&amp;targetType=Credit&amp;countType=Actual&amp;defaultMin=2&amp;defaultIdeal=4&amp;defaultMax=6&amp;manualValidationCheck=Max&amp;errorMessage=Provider%20has%20exceeded%20the%20maximum%20number%20of%20weekend%20shifts&amp;taskTagCategory=Task%20Type&amp;taskTagName=Weekend&amp;locationId=1234")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
