import (
  "fmt"
  "strings"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/task/String/taskshift/GUID/UUID"
  method := "PUT"

  payload := strings.NewReader("effectiveFromDate=DateTime&amp;startTime=DateTime&amp;endTime=DateTime&amp;occurrence=String&amp;statCredit=Decimal&amp;isClearStartTime=Boolean&amp;isClearEndTime=Boolean")

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, payload)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Content-Type", "application/x-www-form-urlencoded")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
