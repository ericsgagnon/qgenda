import (
  "fmt"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/dailycase/?companyKey=00000000-0000-0000-0000-000000000000&amp;startDate=1/12/2012&amp;endDate=1/14/2012&amp;includes=Task,DirectProviders,Supervisors&amp;$select=OData%20Expression&amp;$filter=OData%20Expression&amp;$orderby=OData%20Expression"
  method := "GET"

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, nil)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
