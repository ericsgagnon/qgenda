import (
  "fmt"
  "net/http"
  "io/ioutil"
)

func main() {

  url := "https://api.qgenda.com/v2/staffmember/1234/location/?companyKey=00000000-0000-0000-0000-000000000000&amp;$select=Location/Name,Staff/FirstName,Staff/LastName&amp;$expand=Location,Staff"
  method := "GET"

  client := &amp;http.Client {
  }
  req, err := http.NewRequest(method, url, nil)

  if err != nil {
    fmt.Println(err)
  }
  req.Header.Add("Authorization", "bearer eyJhbGciOiJBMjU2S1ciLCJlbmMiO...")
  req.Header.Add("Content-Type", "application/json")

  res, err := client.Do(req)
  defer res.Body.Close()
  body, err := ioutil.ReadAll(res.Body)

  fmt.Println(string(body))
}</code></pre>
